{
    "settings": {
        "import/resolver": {
          "typescript": {},
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
        }
    },
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        
        "jasmine"
    ],
    "rules": {
        "import/prefer-default-export" : 0,
        "import/extensions" : 0,
        "no-console" : 0,
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
